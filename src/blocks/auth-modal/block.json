    "register"
  ],
  "version": "1.0.0",
  "textdomain": "bwd-plus",
  "editorScript": "file:./index.js",
  "style": "file:./index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "showRegister": {
      "type": "boolean",
      "default": true
    }
  }
}